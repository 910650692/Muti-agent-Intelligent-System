{
  "mcpServers": {
    "12306": {
      "command": "npx",
      "args": [
        "-y",
        "12306-mcp"
      ],
      "description": "12306火车票查询，支持余票查询、过站查询、中转查询",
      "enabled": true
    },
    "bing-cn": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "bing-cn-mcp"
      ],
      "description": "必应中国搜索，支持中文搜索和网页内容获取（无需 API Key）",
      "enabled": false
    },
    "sgm-navigation": {
      "transport": "sse",
      "url": "http://localhost:8080/sse",
      "description": "SGM车载导航服务，支持位置查询、POI搜索、路线规划、导航控制等功能",
      "enabled": true
    }
  }
}
